.page {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  margin-right: 20rpx;
}
.block{
    margin-top: 100rpx;
} 
image{
  padding-right: 10rpx;
}
.title{
  display: flex;
  flex-direction: row;
  color: black;
  font-size: 34rpx;

}
.content{
  display: flex;
  flex-direction: column;
  margin-left:10rpx;
  margin-top: 10rpx;
  color: #707070;
  font-size: 28rpx;
}
button {
  height:100rpx;
  width:350rpx;
  font-size:35rpx;
  background-color:#333333;
  color:#fff;
  line-height:100rpx;
  border-radius:50rpx;
  box-shadow:0 5px 30px 0px rgba(0, 0, 0, 0.3);
  margin-top:100rpx;
}
